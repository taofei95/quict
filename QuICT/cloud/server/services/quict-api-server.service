[Unit]
Description=QuICT Master Node API Server
After=network.target
StartLimitIntervalSec=0

[Service]
Type=forking
Restart=on-failure
RestartSec=5
WorkingDirectory=/home/likaiqi/Workplace/distributed_server
ExecStart=/.local/bin/gunicorn --workers=4 --bind 0.0.0.0:5000 server.main:app

[Install]
WantedBy=default.target